{
  "name": "frontend-template",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "engineStrict": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "watch": "vue-tsc --noEmit && vite build --mode production --watch",
    "build-initial": "npm run lint && vue-tsc --noEmit && vite build --mode initial",
    "preview": "vue-tsc --noEmit && vite preview",
    "deploy": "npm run lint && vue-tsc --noEmit && vite build --mode deploy",
    "deploy-production": "npm run lint && vue-tsc --noEmit && vite build --mode deploy-production",
    "svgo": "svgo ./src/**/*.svg",
    "lint": "eslint --fix && stylelint --fix ."
  },
  "workspaces": [
    "src/*"
  ],
  "browserslist": [
    ">1%",
    "not op_mini all",
    "not ie 11",
    "last 3 chrome version",
    "last 3 firefox version",
    "last 3 safari version"
  ],
  "dependencies": {
    "@popperjs/core": "2.11.8",
    "@vueform/multiselect": "2.6.6",
    "@vueform/slider": "2.1.9",
    "@vueform/toggle": "2.1.4",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "axios": "1.6.1",
    "dialog-polyfill": "0.5.6",
    "gsap": "3.12.2",
    "lodash": "4.17.21",
    "maska": "2.1.10",
    "photoswipe": "5.4.2",
    "pinia": "2.1.7",
    "swiper": "11.0.5",
    "v-calendar": "3.1.2",
    "vue": "3.3.8",
    "vue-awesome-paginate": "1.1.46",
    "vue-router": "4.2.5",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.1.0",
    "@spiriit/vite-plugin-svg-spritemap": "1.4.0",
    "@stylistic/eslint-plugin": "1.0.1",
    "@types/lodash": "4.14.201",
    "@types/node": "20.9.0",
    "@types/pug": "2.0.9",
    "@vitejs/plugin-vue": "4.4.1",
    "@vituum/vite-plugin-juice": "1.1.0",
    "@yandex/ymaps3-types": "latest",
    "autoprefixer": "10.4.16",
    "eslint": "8.53.0",
    "lightningcss": "1.22.1",
    "postcss": "8.4.31",
    "postcss-html": "1.5.0",
    "postcss-sass": "0.5.0",
    "postcss-short": "5.0.0",
    "postcss-sort-media-queries": "5.2.0",
    "pug": "3.0.2",
    "sass": "1.69.5",
    "ssh2-sftp-client": "9.1.0",
    "stylelint": "15.11.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "13.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-order": "6.0.3",
    "sugarss": "4.0.1",
    "svgo": "3.0.3",
    "terser": "5.24.0",
    "typescript": "5.1.3",
    "unplugin-fonts": "1.0.3",
    "vite": "4.5.0",
    "vue-tsc": "1.8.0"
  }
}
